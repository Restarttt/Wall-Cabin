<!--pages/collect/collect.wxml-->
<view class="collect">
	<!-- 列表 -->
	<view class="list" wx:for='{{list}}' wx:key="index">
		<view class="one">
			<image src="  {{item.img}}" />
			<block bindtap="detele">
				<image src="../../images/tag.png" class="tag"/>
			</block>
		</view>
	</view>
	<!-- end -->
	<view class="end">
		<view class="name">
			<view class="line"></view>
			<view class="title">IN YOUR AREA</view>
		</view>
	</view>
</view>